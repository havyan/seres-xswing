<?xml version="1.0" encoding="UTF-8"?>
<xpanel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.seres.com/xsd/xswing" processor="com.xswing.framework.test.TestProcessor">
  <north>
    <borderPanel>
      <north>
        <menuBar>
          <item>
            <menu text="file">
              <item>
                <menu text="open"></menu>
              </item>
              <item>
                <menu text="save"></menu>
              </item>
              <item>
                <menu text="save as"></menu>
              </item>
              <item>
                <menu text="close"></menu>
              </item>
            </menu>
          </item>

          <item>
            <menu text="edit">
              <item>
                <menu text="copy"></menu>
              </item>
              <item>
                <menu text="cut"></menu>
              </item>
              <item>
                <menu text="undo"></menu>
              </item>
              <item>
                <menu text="redo"></menu>
              </item>
            </menu>
          </item>

          <item>
            <menu text="tools">
              <item>
                <menu text="a">
                  <item>
                    <comboBox></comboBox>
                  </item>
                </menu>
              </item>
              <item>
                <menu text="cut"></menu>
              </item>
              <item>
                <menu text="undo"></menu>
              </item>
              <item>
                <menu text="redo"></menu>
              </item>
            </menu>
          </item>

          <item>
            <menu text="run">
              <item>
                <checkBox>
                  <property name="text" value="selected"></property>
                </checkBox>
              </item>
            </menu>
          </item>
        </menuBar>
      </north>
      <south>
        <toolBar>
          <component>
            <button>
              <property name="text" value="new"></property>
            </button>
          </component>
          <component>
            <button>
              <property name="text" value="save"></property>
            </button>
          </component>
          <component>
            <button>
              <property name="text" value="run"></property>
            </button>
          </component>
          <component>
            <button>
              <property name="text" value="run as"></property>
            </button>
          </component>
        </toolBar>
      </south>
    </borderPanel>
  </north>
  <center>
    <borderPanel>
      <north>
        <flowPanel align="LEFT">
          <component>
            <label>
              <property name="text" value="选择项目："></property>
            </label>
          </component>

          <component>
            <comboBox id="project"></comboBox>
          </component>

          <component>
            <button>
              <property name="text" value="新增"></property>
            </button>
          </component>

          <component>
            <button>
              <property name="text" value="修改"></property>
            </button>
          </component>

          <component>
            <button>
              <property name="text" value="删除"></property>
            </button>
          </component>

          <component>
            <button>
              <property name="text" value="复制"></property>
            </button>
          </component>
        </flowPanel>
      </north>
      <center>
        <splitPanel border="ETCHED">
          <before>
            <splitPanel orientation="VERTICAL">
              <before>
                <borderPanel>
                  <north>
                    <menuBar>
                      <item>
                        <button>
                          <property name="text" value="新增"></property>
                        </button>
                      </item>
                      <item>
                        <button>
                          <property name="text" value="修改"></property>
                        </button>
                      </item>
                      <item>
                        <button>
                          <property name="text" value="删除"></property>
                        </button>
                      </item>
                    </menuBar>
                  </north>
                  <center>
                    <scrollPanel>
                      <view>
                        <cbean class="javax.swing.JTree"></cbean>
                      </view>
                    </scrollPanel>
                  </center>
                </borderPanel>
              </before>
              <after>
                <borderPanel>
                  <north>
                    <menuBar>
                      <item>
                        <button>
                          <property name="text" value="新增"></property>
                        </button>
                      </item>
                      <item>
                        <button>
                          <property name="text" value="修改"></property>
                        </button>
                      </item>
                      <item>
                        <button>
                          <property name="text" value="删除"></property>
                        </button>
                      </item>
                    </menuBar>
                  </north>
                  <center>
                    <scrollPanel>
                      <view>
                        <cbean class="javax.swing.JTree"></cbean>
                      </view>
                    </scrollPanel>
                  </center>
                </borderPanel>
              </after>
            </splitPanel>

          </before>
          <after>
            <tabbedPanel>
              <tab title="数据源设置" closable="false">
                <borderPanel name="数据源设置">
                  <center>
                    <boxPanel axis="Y_AXIS">
                      <component>
                        <import path="./datasource.xml" />
                      </component>
                      <component>
                        <gridBagPanel title="数据参数设置">
                          <row>
                            <cell anchor="WEST">
                              <label>
                                <property name="text" value="甘特图名字："></property>
                              </label>
                            </cell>
                            <cell anchor="WEST">
                              <label>
                                <property name="text" value="XX甘特图数据"></property>
                              </label>
                            </cell>
                            <cell anchor="WEST">
                              <label></label>
                            </cell>
                            <cell anchor="WEST">
                              <label></label>
                            </cell>
                          </row>

                          <row>
                            <cell anchor="WEST">
                              <label>
                                <property name="text" value="日期格式："></property>
                              </label>
                            </cell>
                            <cell anchor="WEST" gridwidth="3">
                              <comboBox></comboBox>
                            </cell>
                          </row>
                        </gridBagPanel>
                      </component>

                      <component>
                        <gridBagPanel title="显示参数设置">
                          <row>
                            <cell anchor="WEST">
                              <label>
                                <property name="text" value="甘特图风格："></property>
                              </label>
                            </cell>
                            <cell gridwidth="3" anchor="WEST">
                              <comboBox></comboBox>
                            </cell>
                          </row>
                          <row>
                            <cell anchor="WEST">
                              <label>
                                <property name="text" value="当前时刻："></property>
                              </label>
                            </cell>
                            <cell gridwidth="2" anchor="WEST">
                              <cbean class="com.toedter.calendar.JDateChooser">
                              </cbean>
                            </cell>
                            <cell anchor="NORTHWEST">
                              <button>
                                <property name="text" value="N"></property>
                              </button>
                            </cell>
                          </row>
                          <row>
                            <cell anchor="WEST">
                              <label>
                                <property name="text" value="单元高度："></property>
                              </label>
                            </cell>
                            <cell anchor="WEST">
                              <cbean id="unitHeight" class="javax.swing.JSpinner">
                              </cbean>
                            </cell>

                            <cell anchor="WEST">
                              <label>
                                <property name="text" value="甘特图条高度："></property>
                              </label>
                            </cell>
                            <cell anchor="WEST">
                              <cbean id="barHeight" class="javax.swing.JSpinner">
                              </cbean>
                            </cell>
                          </row>

                          <row>
                            <cell anchor="WEST">
                              <label>
                                <property name="text" value="甘特集宽度："></property>
                              </label>
                            </cell>
                            <cell anchor="WEST">
                              <cbean id="collectionWidth" class="javax.swing.JSpinner">
                              </cbean>
                            </cell>

                            <cell anchor="WEST">
                              <label>
                                <property name="text" value="甘特组宽度："></property>
                              </label>
                            </cell>
                            <cell anchor="WEST">
                              <cbean id="groupWidth" class="javax.swing.JSpinner">
                              </cbean>
                            </cell>
                          </row>
                        </gridBagPanel>
                      </component>

                      <component>
                        <gridBagPanel title="时间标尺设置">
                          <row>
                            <cell anchor="WEST">
                              <label>
                                <property name="text" value="标尺的类型："></property>
                              </label>
                            </cell>
                            <cell anchor="WEST">
                              <comboBox></comboBox>
                            </cell>
                            <cell anchor="WEST">
                              <label>
                                <property name="text" value="缩放比例："></property>
                              </label>
                            </cell>
                            <cell anchor="WEST">
                              <comboBox></comboBox>
                            </cell>
                          </row>
                          <row>
                            <cell anchor="WEST">
                              <label>
                                <property name="text" value="开始时间："></property>
                              </label>
                            </cell>
                            <cell gridwidth="3" anchor="WEST">
                              <cbean class="com.toedter.calendar.JDateChooser">
                              </cbean>
                            </cell>
                          </row>
                          <row>
                            <cell anchor="WEST">
                              <label>
                                <property name="text" value="结束时间："></property>
                              </label>
                            </cell>
                            <cell gridwidth="3" anchor="WEST">
                              <cbean class="com.toedter.calendar.JDateChooser">
                              </cbean>
                            </cell>
                          </row>
                        </gridBagPanel>
                      </component>
                    </boxPanel>
                  </center>

                  <east>
                    <borderPanel>
                      <center>
                        <scrollPanel title="一棵树">
                          <view>
                            <cbean class="javax.swing.JTree">
                            </cbean>
                          </view>
                        </scrollPanel>
                      </center>
                      <east>
                        <scrollPanel title="两棵树">
                          <view>
                            <cbean class="javax.swing.JTree">
                            </cbean>
                          </view>
                        </scrollPanel>
                      </east>
                      <west>
                        <scrollPanel title="三棵树">
                          <view>
                            <cbean class="javax.swing.JTree">
                            </cbean>
                          </view>
                        </scrollPanel>
                      </west>
                    </borderPanel>
                  </east>

                </borderPanel>
              </tab>

              <tab title="欢迎界面">
                <label>
                  <property name="text" value="welcome to xswing"></property>
                </label>
              </tab>

              <tab title="魔幻界面">
                <button>
                  <property name="text" value="welcome to xswing"></property>
                </button>
              </tab>

              <tab title="神奇界面">
                <cbean class="javax.swing.JTree"></cbean>
              </tab>

            </tabbedPanel>
          </after>
        </splitPanel>
      </center>
      <south>
        <label border="LOWEREDBEVEL">
          <property name="text" value="init has be done"></property>
        </label>
      </south>
    </borderPanel>
  </center>
  <bean id="size" class="java.awt.Dimension">
    <argument type="int" value="1000"></argument>
    <argument type="int" value="1000"></argument>
  </bean>
</xpanel>
